    picture
      source(srcset="./img/logo.webp" type="image/webp")
      img(src="./img/logo.png" alt="img")

  div.main-wrap
    .first-screen
      .first-screen__inner
        .first-screen__content#first-screen-slider-js
          .first-screen__item
            .first-screen__item-content
              .first-screen__image.b-lazy.webp-img(data-webp="./img/ati-bunner01.webp"
                data-img="./img/ati-bunner01.jpg" data-0="background-position-y: 100%" data-600="background-position-y: 0%;")
              .first-screen__txt(data-0="opacity: 1;" data-500="opacity: 0;")
                .first-screen__title Дизайн, інженерія, <br> якість
                .first-screen__cupt Внутрішній інженерно-технічний відділ
                .first-screen__desc Вичерпний контроль якості всієї продукції, що постачається.
          .first-screen__item
            .first-screen__item-content
              .first-screen__image.b-lazy.webp-img(data-webp="./img/ati-bunner01.webp"
                data-img="./img/ati-bunner01.jpg" data-0="background-position-y: 100%" data-600="background-position-y: 0%;")
              .first-screen__txt(data-0="opacity: 1;" data-500="opacity: 0;")
                .first-screen__title Дизайн, інженерія, <br> якість
                .first-screen__cupt Внутрішній інженерно-технічний відділ
                .first-screen__desc Вичерпний контроль якості всієї продукції, що постачається.
          .first-screen__item
            .first-screen__item-content
              .first-screen__image.b-lazy.webp-img(data-webp="./img/ati-bunner01.webp"
                data-img="./img/ati-bunner01.jpg" data-0="background-position-y: 100%" data-600="background-position-y: 0%;")
              .first-screen__txt(data-0="opacity: 1;" data-500="opacity: 0;")
                .first-screen__title Дизайн, інженерія, <br> якість
                .first-screen__cupt Внутрішній інженерно-технічний відділ
                .first-screen__desc Вичерпний контроль якості всієї продукції, що постачається.
